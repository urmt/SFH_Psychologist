{
  "name": "sentient-field-psychologist",
  "version": "1.0.0",
  "description": "SFH-compliant AI psychologist for attachment theory and psychedelic integration",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=apps/web",
    "chat": "npm run dev --workspace=apps/simple-chat",
    "build": "npm run build --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "test:integration": "npx tsx test-orchestrator.ts",
    "lint": "npm run lint --workspaces --if-present",
    "deploy:ipfs": "node tools/scripts/deploy-ipfs.js",
    "deploy:vercel": "vercel --prod",
    "vercel:preview": "vercel"
  },
  "devDependencies": {
    "dotenv": "^17.2.3",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "tsx": "^4.20.6",
    "typescript": "^5.3.0",
    "vercel": "^48.10.10"
  },
  "keywords": [
    "sfh",
    "psychology",
    "attachment-theory",
    "psychedelic-integration"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@vercel/node": "^5.5.9",
    "cors": "^2.8.5",
    "express": "^5.1.0"
  }
}
